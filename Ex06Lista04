#include <stdio.h>
/* FaÃ§a um programa que recebe um valor de 2 a 9 e retorna na tela a tabuada desse valor. */
int main(void) {
  int a,i,j,x;
  printf("Digite um numero de 2 a 9: ");
  scanf("%d", &a);
  if( a >=2 && a<=9){
    for(i=0;i<=9;i++){
      x = i*a;
      printf("%d x %d = %d\n", a,i,x);
     
    }
  }else {
    printf("Escolha um numero entre os parametros!!!!!!");
  }
  
   
  return 0;
}
